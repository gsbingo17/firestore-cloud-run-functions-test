{
  "name": "firestore-trigger-test",
  "version": "1.0.0",
  "description": "Cloud Run Functions with Firestore trigger latency test",
  "main": "index.js",
  "scripts": {
    "start-first": "functions-framework --target=modifyDocument --source=src/firstFunction.js --port=8080 --signature-type=http --security-level=none",
    "start-second": "functions-framework --target=onItemChange --source=src/secondFunction.js --port=8081 --signature-type=http --security-level=none",
    "test-local": "GOOGLE_APPLICATION_CREDENTIALS=./service-account.json node test/test-functions.js http://localhost:8080",
    "test-prod": "node test/test-functions.js"
  },
